{
  "expo": {
    "name": "chat_app",
    "slug": "chat_app",
    "version": "1.0.0",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "web": {
      "bundler": "metro",
      "output": "static"
    },
    "plugins": [
      "expo-router",
      "expo-secure-store"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "scheme": "myapp",
    "ios": {
      "bundleIdentifier": "com.fazitech.chat-app"
    },
    "android": {
      "package": "com.fazitech.chatapp"
    }
  }
}
